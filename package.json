    "json-loader": "0.5.7",
    "node-sass": "^7.0.1",
    "object-assign": "4.1.1",
    "path-exists": "5.0.0",
    "postcss-loader": "7.0.0",
    "promise": "8.1.0",
    "react-dev-utils": "^12.0.1",
    "recursive-readdir": "2.2.2",
    "sass-loader": "^13.0.0",
    "strip-ansi": "7.0.1",
